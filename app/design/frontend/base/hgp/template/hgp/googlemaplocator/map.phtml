    function load() {
        if (GBrowserIsCompatible()) {
      	map = new GMap2(document.getElementById("map"));
      	
      	map.setCenter(new GLatLng(<?php echo $_latitude;?>, <?php echo $_longitude;?>), <?php echo $_zoom;?>);

          map.addControl(new GMapTypeControl()); // Differentes vues (plan, satellite, mixte)
          map.addMapType(G_PHYSICAL_MAP);        // Ajout de la vue en "Relief"
          map.setMapType(G_NORMAL_MAP);          // Forcer sur vue "mixte": G_HYBRID_MAP
          map.addControl(new GLargeMapControl3D());  // Zoom large 3D
          /*map.enableScrollWheelZoom();           // Activation du zoom avec la souris*/
          map.addControl(new GScaleControl());   // Echelle      

         GDownloadUrl("<?php echo $mediaPath . 'markerdata_'.$storeId .'.xml'?>", function(data) { 
            var xml = GXml.parse(data);
            console.log("XML =>", xml);
            var markers = xml.documentElement.getElementsByTagName("marker");
            for (var i = 0; i < markers.length; i++) {
              var name = markers[i].getAttribute("name");
              var label = markers[i].getAttribute("label");
              var address = markers[i].getAttribute("address");
              var cp = markers[i].getAttribute("cp");
              var ville = markers[i].getAttribute("ville");
              var type = markers[i].getAttribute("type");
              var url = markers[i].getAttribute("url");
              var css = markers[i].getAttribute("css");
              var ref = markers[i].getAttribute("ref");
              var ad = markers[i].getAttribute("ad");
              var point = new GLatLng(parseFloat(markers[i].getAttribute("lat")), parseFloat(markers[i].getAttribute("lng")));
              var marker = createMarker(point, name, label, address, cp, ville, type, url, css, ref, ad);
             
              map.addOverlay(marker);
              initCheckbox();
            }
          });    
        }
      }

  	function initCheckbox(){
