          document.getElementById("hotelCheckbox").checked = true;
          document.getElementById("poiCheckbox").checked = false;
          document.getElementById("seeCheckbox").checked = false;
          document.getElementById("drinkCheckbox").checked = false;
          document.getElementById("nightCheckbox").checked = false;
  	}

      function createMarker(point, name, label, address, cp, ville, type, url, css, ref, ad) {
  		
          var marker = new LabeledMarker(point, {icon: customIcons[type], labelText: label, labelOffset: new GSize(-11, -17), labelClass: css});
        markerGroups[type].push(marker);
        if (url != '#'){
        	var html = "<img width='100' height='100' class='pict-map' src='<?php echo ($mediaPath . 'visuels_hotels/') ; ?>" + ref + ".jpg' alt=''/><div id='info_bulle_global'><span id='name_infobulle'>" + name + "</span><span id='ad_infobulle'>" + ad + "</span><span id='adress_infobulle'>" + address + "</span> <br/> <span id='cp_infobulle'>" + cp + " " + ville + "</span> <br/><a href='"+ url +"' class='link'><?php echo $this->__('See this hotel')?></a></div>";
        } else {
        	var html = "<span>" + name + "</span> <br/> <span id='adress_infobulle'>" + address + "</span> <br/><span id='cp_infobulle'>" + cp + " " + ville + "</span> <br/>";
        }
        GEvent.addListener(marker, 'mouseover', function() {
          marker.openInfoWindowHtml(html);
        });
        return marker;
      }
 
    function toggleGroup(type) {
        for (var i = 0; i < markerGroups[type].length; i++) {
        var marker = markerGroups[type][i];
        if (marker.isHidden()) {
          marker.show();
        } else {
          marker.hide();
        }
      } 
    }
    
    function unCheckPOI() {
    	var type = 'poi';
        for (var i = 0; i < markerGroups[type].length; i++) {
        var marker = markerGroups[type][i];
		marker.hide();
      } 
    }

    function unCheckSEE() {
    	var type = 'see';
        for (var i = 0; i < markerGroups[type].length; i++) {
        var marker = markerGroups[type][i];
		marker.hide();
      } 
    }

    function unCheckDRINK() {
    	var type = 'drink';
        for (var i = 0; i < markerGroups[type].length; i++) {
        var marker = markerGroups[type][i];
		marker.hide();
      } 
    }

    function unCheckNIGHT() {
    	var type = 'night';
        for (var i = 0; i < markerGroups[type].length; i++) {
        var marker = markerGroups[type][i];
		marker.hide();
      } 
    }

    function toggleLabels() {
      var showLabels = document.getElementById("labelsCheckbox").checked;
      for (groupName in markerGroups) {
        for (var i = 0; i < markerGroups[groupName].length; i++) {
          var marker = markerGroups[groupName][i];
          marker.setLabelVisibility(showLabels);
        }
      }
    }   

	function setMap(e) {

		if (map.currentMarker){
			map.removeOverlay(map.currentMarker);
		}
		var point = new GLatLng(e.y,e.x) ;
	 	map.setCenter( point, 15);
		var iconCurrentAddress = new GIcon(G_DEFAULT_ICON);
		markerOptions = { icon:iconCurrentAddress };
		
		map.currentMarker = new GMarker(point, markerOptions)
		map.addOverlay(map.currentMarker);
		var infoWindowCurrentAddress = map.openInfoWindowHtml(point,'Votre recherche : ' + queryAddress.value);
		
		 GEvent.addListener(map.currentMarker, "click", function(){map.openInfoWindowHtml(point,'Votre recherche : ' + queryAddress.value)});
				
	}
	
    function search(){
		geoCoder = new GClientGeocoder();
		queryAddress = document.getElementById('queryInput');
		geoCoder.getLatLng(queryAddress.value, setMap)
		return false;
